<Datatype, dtype::str>


#[name=boolean, run=interpreter]>

testutil::print(dtype::of_(true) == dtype::bool);
testutil::print(dtype::of_(false) == dtype::bool);
testutil::print(dtype::of_(true) == dtype::str);
testutil::print(dtype::of_(true));
testutil::print(dtype::of_(false));

---

true
true
false
