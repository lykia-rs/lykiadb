#[name=numerical, run=interpreter]>

testutil::print(dtype::of_(1) == dtype::num);
testutil::print(dtype::of_(1.12) == dtype::num);
testutil::print(dtype::of_(1) == dtype::str);
testutil::print(dtype::of_(1));
testutil::print(dtype::of_(1.12));
---

