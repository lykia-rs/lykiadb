#[name=array, run=interpreter]>
testutil::print(dtype::of_([1, 2, 3]) == dtype::array(dtype::num));
testutil::print(dtype::of_([1, 2, 3]) == dtype::array(dtype::str));
testutil::print(dtype::of_([1, 2, 3]));

---

true
false
<Datatype, dtype::array(dtype::num)>


#[name=object, run=interpreter]>
testutil::print(dtype::of_({a: 1, b: 2}) == dtype::object({a: dtype::num, b: dtype::num}));
testutil::print(dtype::of_({a: [1, 2, 3] }) == dtype::object({a: dtype::array(dtype::num)}));
testutil::print(dtype::of_({a: [1, 2, 3] }));
testutil::print(dtype::of_({a: 1, b: 2}));
---

true
true
<Datatype, dtype::object({a: dtype::array(dtype::num)})>
<Datatype, dtype::object({b: dtype::num, a: dtype::num})>


#[name=callable, run=interpreter]>

testutil::print(dtype::of_(out::print) == dtype::str);
testutil::print(dtype::of_(out::print));

---

false
<Datatype, dtype::callable(dtype::unknown, dtype::unknown)>


