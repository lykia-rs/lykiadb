#[name=loop_statements_1, run=interpreter]>

for (var $i = 0; $i < 2; $i = $i + 1) {
    for (var $j = 0; $j < 3; $j = $j + 1) {
        testutil::print($i + ":" + $j);        
    }
}

---

0:0
0:1
0:2
1:0
1:1
1:2

#[name=loop_statements_2, run=interpreter]>

var $q = 0;

for (var $i = 0; $i < 5; $i = $i + 1) {
    $q = $q + 1;
    {
        {
            {
                {
                    {
                        {
                            {
                                testutil::print($q);
                            }
                        }
                    }
                }
            }
        }
    }
}


