#[name=anonymous_fn_0, run=interpreter]>

var $pr = function $a() {
    testutil::print("hello");
};

$pr();
$a();

---

hello
hello


#[name=anonymous_fn_1_iife, run=interpreter]>

(function $a() {
    testutil::print("hello");
})();

$a();

---

hello
hello


#[name=anonymous_fn_2, run=interpreter]>

