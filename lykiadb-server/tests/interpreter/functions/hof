#[name=higher_order_0, run=interpreter]>

function $f($x: dtype::callable(dtype::tuple(dtype::str), dtype::unit), $q: dtype::num) {
    $x($q);
};

function $g($q: dtype::str) {
    test_utils::out($q);
};

for (var $i = 0; $i < 10; $i = $i + 1) {
    $f($g, $i);
}

