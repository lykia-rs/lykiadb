// Define recursive Fibonacci function
function $fib($n) {
  if ($n < 2) return $n;
  return $fib($n - 2) + $fib($n - 1);
};

var $start_ly = time::clock();
out::print($fib(35) == 9227465);
out::print("elapsed (user defined):", time::clock() - $start_ly);

var $start_rs = time::clock();
out::print(bench::fib(35) == 9227465);
out::print("elapsed (native):", time::clock() - $start_rs);