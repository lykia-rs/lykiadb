var $calc = {
  add: function ($a: dtype::num, $b: dtype::num) -> dtype::num {
    return $a + $b;
  },
  sub: function ($a: dtype::num, $b: dtype::num) -> dtype::num {
    return $a - $b;
  },
  mul: function ($a: dtype::num, $b: dtype::num) -> dtype::num {
    return $a * $b;
  },
  div: function ($a: dtype::num, $b: dtype::num) -> dtype::num {
    return $a / $b;
  },
};
io::print($calc.add(4, 5));
io::print($calc.sub(4, 5));
io::print($calc.mul(4, 5));
io::print($calc.div(4, 5));

